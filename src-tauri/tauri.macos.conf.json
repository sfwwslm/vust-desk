{
  "$schema": "https://schema.tauri.app/config/2",
  "identifier": "cc.vust.desk",
  "app": {
    "enableGTKAppId": false,
    "macOSPrivateApi": true,
    "security": {
      "assetProtocol": {
        "enable": true,
        "scope": ["$HOME/.vust/**"]
      },
      "capabilities": [],
      "dangerousDisableAssetCspModification": false,
      "freezePrototype": false,
      "pattern": {
        "use": "brownfield"
      }
    },
    "windows": [
      {
        "title": "Vust Desk",
        "width": 1024,
        "height": 768,
        "minWidth": 1024,
        "minHeight": 768,
        "decorations": true,
        "visible": false,
        "transparent": false
      }
    ],
    "withGlobalTauri": false
  },
  "plugins": {
    "fs": {
      "requireLiteralLeadingDot": false
    }
  }
}
